fun factorial(n) {
    var product = 1;
    for(var i = 2; i < n; i = i+1) {
        product = product * i;
    }
    return product;
}


fun fibonacci(n) {
    if n < 3 {
        return 1;
    } else {
        return fibonacci(n-1) + fibonacci(n-2);
    }
}


fun main() {
    print fibonacci(6);
    print factorial(6);
}
